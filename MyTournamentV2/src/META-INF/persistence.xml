<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1"
	xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
	<persistence-unit name="MyTournamentV2"
		transaction-type="JTA">
		<provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
		<jta-data-source>java:comp/env/jdbc/MyTournamentV2</jta-data-source>
		<class>mt.objects.Clan</class>
		<class>mt.objects.Formattournament</class>
		<class>mt.objects.Gameaccount</class>
		<class>mt.objects.Game</class>
		<class>mt.objects.Location</class>
		<class>mt.objects.Reward</class>
		<class>mt.objects.Selection</class>
		<class>mt.objects.Sponsor</class>
		<class>mt.objects.Tournament</class>
		<class>mt.objects.Typegameaccount</class>
		<class>mt.objects.Typeofreward</class>
		<class>mt.objects.Typeoftournament</class>
		<class>mt.objects.Unavailability</class>
		<class>mt.objects.User</class>
		<class>mt.objects.Usersstatut</class>
		<class>mt.objects.Webref</class>

		<properties>
			<property name="eclipselink.ddl-generation.output-mode"	value="database" />
			<property name="eclipselink.ddl-generation" value="create-tables" />
		</properties>
	</persistence-unit>
</persistence>
